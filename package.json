{
  "name": "test-vue3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode development",
    "build-lib": "vue-cli-service build --target lib --mode development --name Employe ./src/components/Employe.vue",
    "build-ce": "vue-cli-service build --target lib --mode development --name Test ./src/components/test.js",
    "test:unit": "vue-cli-service test:unit --watchAll --coverage",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "core-js": "^3.16.1",
    "jshint": "^2.13.1",
    "roboto-fontface": "*",
    "vue": "^3.2.2",
    "vuetify": "^3.0.0-alpha.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.2",
    "@vue/test-utils": "^2.0.0-rc.12",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.16.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "^3.9.10",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
